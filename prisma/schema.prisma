generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("NEXT_PUBLIC_DATABASE_URL")
}

model Post {
  id        Int     @id @default(autoincrement())
  title     String
  body      String
  tags      Tag[]
}

model Tag {
  id        Int     @id @default(autoincrement())
  slug      String  @unique
  title     String
  posts     Post[]
}
